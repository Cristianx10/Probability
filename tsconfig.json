{
  "compilerOptions": {
    "module": "ESNext",
    "target": "es5",
    "lib": [
      "es5",
      "es2015",
      "scripthost",

      "dom",
      "dom.iterable",
      "esnext",
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}